{
  "name": "vsc-workspace-sidebar",
  "description": "Adds a sidebar to VSCode that lists Workspaces and lets you open them in the current window or a new window.",
  "displayName": "Workspace Sidebar",
  "version": "1.6.1",
  "activationEvents": [
    "onStartupFinished"
  ],
  "bugs": {
    "url": "https://github.com/sketchbuch/vsc-workspace-sidebar/issues"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vsc-workspace-sidebar-open-cur-win",
        "title": "%ext.cmd.wsListItem.open.curWindow%"
      },
      {
        "command": "vsc-workspace-sidebar-open-new-win",
        "icon": {
          "light": "./resources/images/light/new-window.svg",
          "dark": "./resources/images/dark/new-window.svg"
        },
        "title": "%ext.cmd.wsListItem.open.newWindow%"
      },
      {
        "command": "vsc-workspace-sidebar-refresh",
        "icon": {
          "light": "./resources/images/light/refresh.svg",
          "dark": "./resources/images/dark/refresh.svg"
        },
        "title": "%ext.cmd.wsList.refresh%"
      },
      {
        "command": "vsc-workspace-sidebar-settings",
        "title": "%ext.cmd.wsListItem.checkSettings%"
      },
      {
        "command": "vsc-workspace-sidebar-sort",
        "icon": {
          "light": "./resources/images/light/sort.svg",
          "dark": "./resources/images/dark/sort.svg"
        },
        "title": "%ext.cmd.wsList.sort%"
      },
      {
        "command": "vsc-workspace-sidebar-collapse-all",
        "icon": {
          "light": "./resources/images/light/collapse-all.svg",
          "dark": "./resources/images/dark/collapse-all.svg"
        },
        "title": "%ext.cmd.wsList.collapse-all%"
      },
      {
        "command": "vsc-workspace-sidebar-expand-all",
        "icon": {
          "light": "./resources/images/light/expand-all.svg",
          "dark": "./resources/images/dark/expand-all.svg"
        },
        "title": "%ext.cmd.wsList.expand-all%"
      }
    ],
    "configuration": [
      {
        "title": "%config.title%",
        "properties": {
          "workspaceSidebar.actions": {
            "default": "Current Window",
            "description": "%config.workspaceActions.description%",
            "enum": [
              "Current Window",
              "New Window"
            ],
            "enumDescriptions": [
              "%config.workspaceActions.cur-window%",
              "%config.workspaceActions.new-window%"
            ],
            "scope": "application",
            "type": "string"
          },
          "workspaceSidebar.cleanLabels": {
            "default": true,
            "description": "%config.workspaceCleanLabels.description%",
            "scope": "application",
            "type": "boolean"
          },
          "workspaceSidebar.depth": {
            "default": 0,
            "description": "%config.workspaceFolderDepth.description%",
            "maximum": 25,
            "minimum": 0,
            "scope": "application",
            "type": "number"
          },
          "workspaceSidebar.showFileIcons": {
            "default": true,
            "markdownDescription": "%config.workspaceShowFileIcons.description%",
            "scope": "application",
            "type": "boolean"
          },
          "workspaceSidebar.showFileIconsConfig": {
            "default": {},
            "markdownDescription": "%config.workspaceShowFileIconsConfig.description%",
            "scope": "application",
            "type": "object"
          }
        }
      },
      {
        "title": "%config.title.folders%",
        "properties": {
          "workspaceSidebar.folder": {
            "default": "",
            "description": "%config.workspaceFolder.description%",
            "scope": "application",
            "type": "string"
          },
          "workspaceSidebar.folders.excluded": {
            "default": [
              "node_modules",
              "build",
              "dist",
              "out",
              "public"
            ],
            "description": "%config.workspaceSidebar.folders.excluded.description%",
            "scope": "application",
            "type": "array"
          }
        }
      },
      {
        "title": "%config.title.search%",
        "properties": {
          "workspaceSidebar.searchMinimum": {
            "default": 15,
            "description": "%config.workspaceSearchMinimum.description%",
            "maximum": 100,
            "minimum": 0,
            "scope": "application",
            "type": "number"
          },
          "workspaceSidebar.search.caseInsensitive": {
            "default": false,
            "description": "%config.workspace.search.caseInsensitive.description%",
            "scope": "application",
            "type": "boolean"
          },
          "workspaceSidebar.search.matchStart": {
            "default": false,
            "description": "%config.workspace.search.matchStart.description%",
            "scope": "application",
            "type": "boolean"
          }
        }
      },
      {
        "title": "%config.title.listview%",
        "properties": {
          "workspaceSidebar.showPaths": {
            "default": "As needed",
            "description": "%config.workspaceShowPath.description%",
            "enum": [
              "Always",
              "As needed",
              "Never"
            ],
            "enumDescriptions": [
              "%config.workspaceShowPath.always%",
              "%config.workspaceShowPath.asneeded%",
              "%config.workspaceShowPath.never%"
            ],
            "scope": "application",
            "type": "string"
          }
        }
      },
      {
        "title": "%config.title.treeview%",
        "properties": {
          "workspaceSidebar.showFolderHierarchy": {
            "default": false,
            "description": "%config.workspaceShowFolderHierarchy.description%",
            "scope": "application",
            "type": "boolean"
          },
          "workspaceSidebar.showRootFolder": {
            "default": false,
            "description": "Show root folder",
            "scope": "application",
            "type": "boolean"
          },
          "workspaceSidebar.condenseFileTree": {
            "default": true,
            "description": "%config.workspaceCondenseFileTree.description%",
            "scope": "application",
            "type": "boolean"
          }
        }
      }
    ],
    "keybindings": [
      {
        "command": "vsc-workspace-sidebar-focus-search",
        "key": "ctrl+shift+q",
        "mac": "cmd+shift+q",
        "when": "sideBarVisible && focusedView == vsc-workspace-sidebar-webview-workspace"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "vsc-workspace-sidebar-collapse-all",
          "group": "navigation@0",
          "when": "view == vsc-workspace-sidebar-webview-workspace && vsc-workspace-sidebar-list-loaded && config.workspaceSidebar.showFolderHierarchy"
        },
        {
          "command": "vsc-workspace-sidebar-expand-all",
          "group": "navigation@1",
          "when": "view == vsc-workspace-sidebar-webview-workspace && vsc-workspace-sidebar-list-loaded && config.workspaceSidebar.showFolderHierarchy"
        },
        {
          "command": "vsc-workspace-sidebar-sort",
          "group": "navigation@2",
          "when": "view == vsc-workspace-sidebar-webview-workspace && vsc-workspace-sidebar-list-loaded && !config.workspaceSidebar.showFolderHierarchy"
        },
        {
          "command": "vsc-workspace-sidebar-refresh",
          "group": "navigation@3",
          "when": "view == vsc-workspace-sidebar-webview-workspace && vsc-workspace-sidebar-list-loaded"
        }
      ]
    },
    "views": {
      "vsc-workspace-sidebar": [
        {
          "type": "webview",
          "id": "vsc-workspace-sidebar-webview-workspace",
          "name": "%views.title%"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "icon": "./resources/images/activitybar.svg",
          "id": "vsc-workspace-sidebar",
          "title": "%viewsContainers.activitybar.title%"
        }
      ]
    }
  },
  "engines": {
    "vscode": "^1.53.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "lint-staged",
      "pre-commit": "lint-staged"
    }
  },
  "icon": "logo.png",
  "keywords": [
    "workspaces"
  ],
  "license": "SEE LICENSE IN LICENSE.txt",
  "lint-staged": {
    "{src, resources/js}/**/*.{js,ts}": [
      "yarn lint:ts",
      "yarn lint:prettier",
      "git add"
    ]
  },
  "main": "./dist/extension.js",
  "publisher": "sketchbuch",
  "repository": {
    "type": "git",
    "url": "https://github.com/sketchbuch/vsc-workspace-sidebar"
  },
  "scripts": {
    "build:dev": "parcel build ./src/extension.ts --target=dev",
    "build:release": "parcel build ./src/extension.ts --target=release",
    "clean:cache": "rimraf ./.parcel-cache/*",
    "clean:dist": "rimraf ./dist/*",
    "clean:test": "rimraf ./test/*",
    "clean": "yarn clean:cache && yarn clean:dist && yarn clean:test",
    "dev": "yarn clean:dist && yarn build:dev",
    "lint:css": "stylelint --config=.stylelintrc './resources/css/**/*.css'",
    "lint:js": "eslint ./resources/js --ext js --fix",
    "lint:prettier": "prettier --write --config ./.prettierrc.js './{src, resources/js}/**/*.{js,ts}'",
    "lint:ts": "eslint ./src --ext ts --fix",
    "lint": "yarn lint:js && yarn lint:ts && yarn lint:prettier && yarn lint:css",
    "pretest": "yarn clean:dist && yarn build:dev && yarn lint",
    "test": "yarn clean:test && yarn typecheck",
    "test:mocha": "mocha -w -ui tdd --",
    "typecheck": "tsc -p ./",
    "typecheck:mocha": "tsc -w -p ./",
    "vscode:prepublish": "yarn clean:dist && yarn build:release"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "@vscode/codicons": "^0.0.29",
    "@vscode/webview-ui-toolkit": "^1.2.2",
    "crypto": "^1.0.1",
    "json5": "^2.2.3",
    "vscode-ext-localisation": "^1.1.0",
    "vscode-file-theme-processor": "^1.0.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "@types/chai": "^4.2.14",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/mock-fs": "^4.13.0",
    "@types/node": "^13.11.0",
    "@types/sinon": "^9.0.9",
    "@types/vscode": "^1.50.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "chai": "^4.2.0",
    "commitlint": "^8.3.5",
    "eslint": "^6.8.0",
    "eslint-plugin-mocha": "^10.1.0",
    "glob": "^7.1.6",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "mocha": "^7.1.2",
    "mock-fs": "^4.13.0",
    "parcel": "^1.12.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.2",
    "stylelint": "^14.7.1",
    "stylelint-config-recommended": "^7.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^4.9.3",
    "vscode-test": "^1.3.0"
  }
}
